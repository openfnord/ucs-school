#: umc/python/printermoderation/__init__.py:278
#, python-format
msgid "Failed to connect to print server %(printserver)s."
msgstr "Échec de la connexion au serveur d'impression %(printserver)s."

#: umc/python/printermoderation/__init__.py:288
#, python-format
msgid "Failed to print on %(printer)s: %(stderr)s (error %(errno)d)."
msgstr "Échec de l'impression sur %(printer)s : %(stderr)s (erreur %(errno)d)."

#: umc/python/printermoderation/__init__.py:265
#, python-format
msgid "File %r could not be printed as it does not exist (anymore)."
msgstr "Impossible d'imprimer le fichier « %r », car il n'existe pas (plus)."

#: umc/python/printermoderation/__init__.py:97
#: umc/python/printermoderation/__init__.py:203
msgid "Invalid file"
msgstr "Fichier non valable"

#: umc/python/printermoderation/__init__.py:261
msgid "Invalid printer URI"
msgstr "URI de l'imprimante non valable"

#: umc/python/printermoderation/__init__.py:284
msgid "No permission to print"
msgstr "Aucun droit d'impression"

#: umc/python/printermoderation/__init__.py:285
msgid "Wrong password"
msgstr "Mot de passe erroné"
