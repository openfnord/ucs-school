#: umc/python/distribution/util.py:400
msgid "A valid project owner needs to be specified."
msgstr "Für ein gültiges Projekt muss ein Besitzer definiert sein."

#: umc/python/distribution/util.py:431
#, python-format
msgid "Could not save project file: %s (%s)"
msgstr "Das Projekt konnte nicht gespeichert werden: %s (%s)"

#: umc/python/distribution/__init__.py:240
#, python-format
msgid "Could not set date for: %s"
msgstr "Die Zeit konnte nicht gesetzt werden: %s"

#: umc/python/distribution/__init__.py:256
msgid ""
"Distributing the data needs to happen sufficiently long enough before "
"collecting them"
msgstr ""
"Die Verteilung der Daten muss ausreichend lange vor ihrem Einsammeln "
"geschehen"

#: umc/python/distribution/__init__.py:181
#, python-format
msgid "Failed to load user information: %s"
msgstr "Das Laden der Benutzerinformationen schlug fehl: %s"

#: umc/python/distribution/__init__.py:523
msgid "Only the owner himself or an administrator may delete a project."
msgstr ""
"Nur der Eigentümer selbst oder ein Administrator können ein Projekt löschen."

#: umc/python/distribution/__init__.py:428
msgid "Only the owner himself or an administrator may distribute a project."
msgstr ""
"Nur der Eigentümer selbst oder ein Administrator können ein Projekt "
"austeilen."

#: umc/python/distribution/__init__.py:422
#: umc/python/distribution/__init__.py:464
#: umc/python/distribution/__init__.py:501
#, python-format
msgid "Project \"%s\" could not be loaded"
msgstr "Das Projekt \"%s\" konnte nicht geladen werden"

#: umc/python/distribution/__init__.py:228
msgid "Project collection"
msgstr "Projekteinsammlung"

#: umc/python/distribution/__init__.py:368
msgid ""
"Project details are only visible to the project owner himself or an "
"administrator."
msgstr ""
"Projekteigenschaften sind nur sichtbar für den Eigentümer selbst oder einen "
"Administrator."

#: umc/python/distribution/__init__.py:227
msgid "Project distribution"
msgstr "Projektverteilung"

#: umc/python/distribution/__init__.py:478
#, python-format
msgid "The following user directories could not been collected: %s"
msgstr ""
"Die folgenden Benutzer-Verzeichnisse konnten nicht eingesammelt werden: %s"

#: umc/python/distribution/util.py:398
msgid "The given project description must be non-empty."
msgstr "Die angegebene Projektbeschreibung darf nicht leer sein."

#: umc/python/distribution/util.py:381
msgid "The given project directory name must be non-empty."
msgstr "Das angegebene Projektverzeichnis darf nicht leer sein."

#: umc/python/distribution/__init__.py:223
msgid "The project can only be modified by the owner himself"
msgstr "Ein Projekt kann nur durch den Eigentümer selbst bearbeitet werden"

#: umc/python/distribution/__init__.py:323
#: umc/python/distribution/__init__.py:440
#, python-format
msgid "The project could not distributed to the following users: %s"
msgstr ""
"Das Projekt konnte nicht an die folgenden Benutzer ausgeteilt werden: %s"

#: umc/python/distribution/util.py:396
msgid "The specified project directory may at most be 254 characters long."
msgstr "Das angegebene Projektverzeichnis darf maximal 254 Zeichen lang sein"

#: umc/python/distribution/util.py:387
#, python-format
msgid "The specified project directory may not contain the character \"%s\"."
msgstr ""
"Das angegebene Projektverzeichnis darf nicht das Zeichen \"%s\" enthalten."

#: umc/python/distribution/util.py:392
msgid "The specified project directory may not start nor end with a \".\"."
msgstr ""
"Das angegebene Projektverzeichnis darf nicht mit einem \".\" beginnen oder "
"enden."

#: umc/python/distribution/util.py:394
msgid "The specified project directory may not start nor end with a space."
msgstr ""
"Das angegebene Projektverzeichnis darf nicht mit einem Leerzeichen beginnen oder "
"enden."

#: umc/python/distribution/util.py:390
msgid ""
"The specified project directory must be different from \".\" and \"..\"."
msgstr ""
"Das angegebene Projektverzeichnis muss verschieden sein von \".\" and \"..\"."

#: umc/python/distribution/__init__.py:285
#, python-format
msgid ""
"The specified project directory name \"%s\" is already in use by a different "
"project."
msgstr "Das angegebene Projektverzeichnis \"%s\" wird bereits verwendet."

#: umc/python/distribution/__init__.py:212
#, python-format
msgid "The specified project does not exist: %s"
msgstr "Das angegebene Projekt existiert nicht: %s"

#: umc/python/distribution/__init__.py:245
#, python-format
msgid "The specified time needs to lie in the future for: %s"
msgstr "Die angegebene Zeit muss in der Zukunft liegen: %s"

