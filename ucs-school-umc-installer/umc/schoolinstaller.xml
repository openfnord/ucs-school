<?xml version="1.0" encoding="UTF-8"?>
<umc version="2.0">
  <module id="schoolinstaller" icon="schoolinstaller" version="1.0" python="3">
	<name>UCS@school configuration wizard</name>
	<description>This configuration wizard guides the basic configuration of UCS@school in the domain</description>
	<categories>
	  <category name="ucs-school-admin"/>
	</categories>

	<command name="schoolinstaller/query" function="query"/>
	<command name="schoolinstaller/install" function="install"/>
	<command name="schoolinstaller/credentials" function="credentials"/>
	<command name="schoolinstaller/progress" function="progress"/>
	<command name="schoolinstaller/get/schoolinfo" function="get_schoolinfo"/>
	<command name="schoolinstaller/get/metainfo" function="get_metainfo"/>
	<command name="schoolinstaller/get/schoolinfo/master" function="get_schoolinfo_master"/>
	<command name="schoolinstaller/get/metainfo/master" function="get_metainfo_master"/>
  </module>
</umc>
